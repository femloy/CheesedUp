function check_boss(obj_index)
{
	return obj_index == obj_pepperman or obj_index == obj_noiseboss or obj_index == obj_vigilanteboss
	or obj_index == obj_fakepepboss or obj_index == obj_pf_fakepep or obj_index == obj_pizzafaceboss
	or obj_index == obj_pizzafaceboss_p3 or obj_index == obj_pizzafaceboss_p2;
}
